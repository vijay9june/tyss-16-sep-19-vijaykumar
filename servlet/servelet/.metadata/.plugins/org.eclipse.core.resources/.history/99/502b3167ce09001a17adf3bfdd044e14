<%!
	public int i = 10;
	public void m() {
		System.out.println(i);
	}
%>
<!DOCTYPE html>
<%@page import="java.util.Date"%>
<html>
<head>
<meta charset="ISO-8859-1">
<title>Insert title here</title>
</head>
<%!
	private void m1() {
	System.out.println("this is m1()");
}
%>
<%
	Date date = new Date();
	m();
%>

<body>
	<h1 style="color:green;">Date and Time is <%=date%></h1>
	<h3><%= i %></h3>
</body>
